config_version=5

[application]

config/name="Alostn Bridge Diddle School"
config/description="ABDS the game"
run/main_scene="res://scenes/logos.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/image="res://textures/azzuNotFound.png"
config/icon="res://textures/azzuNotFound.png"
config/windows_native_icon="res://favicon.ico"

[audio]

driver/enable_input=true
driver/mix_rate=8000

[autoload]

GlobalVars="*res://scripts/global_vars.gd"
Settings="*res://scripts/AudioVolume.gd"
Game="*res://scripts/game_singleton.gd"
GuiManager="*res://scripts/GUIManager.gd"
SteamManager="*res://scenes/steam_manager.tscn"
Achievements="*res://scenes/AchievementsOverlay.tscn"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/unused_variable=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="uid://djlehyq2xco8v"

[editor_plugins]

enabled=PackedStringArray("res://addons/dev_time_tracker/plugin.cfg")

[filesystem]

import/fbx/enabled=false

