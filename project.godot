;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Alostn Bridge Diddle School"
config/description="Alostn Bridge Diddle School"
run/main_scene="res://logos.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://azzuNotFound.png"
boot_splash/fullsize=false
config/icon="res://azzuNotFound.png"
config/windows_native_icon="res://favicon.ico"
boot_splash/minimum_display_time=1000

[audio]

driver/enable_input=true
driver/mix_rate=8000

[autoload]

EndGameSingleton="*res://end_game_singleton.gd"
Allsingleton="*res://allsingleton.gd"
Settings="*res://AudioVolume.gd"
Networking="*res://networking.gd"
Achievements="*res://Achievements.gd"
LiveSplit="*res://LiveSplit.gd"
Idk="*res://idk.gd"
Leaderboard="*res://Leaderboard.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=800
window/size/window_height_override=600
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/csgtomeshinstance/plugin.cfg", "res://addons/gdscript_formatter/plugin.cfg")

[filesystem]

import/fbx/enabled=false

